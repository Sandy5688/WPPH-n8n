{
  "name": "Mock Data to WordPress (Webhook Response)",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "GET",
        "path": "mock-to-wp",
        "responseMode": "responseNode",
        "options": {}
      },
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [0, 300]
    },
    {
      "parameters": {
        "url": "http://nginx/mock-data.json",
        "responseFormat": "json"
      },
      "name": "Fetch Mock Data",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "responseMode": "lastNode",
        "responseData": "allEntries"
      },
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [500, 300]
    }
  ],
  "connections": {
    "Webhook Trigger": {
      "main": [
        [
          {
            "node": "Fetch Mock Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Mock Data": {
      "main": [
        [
          {
            "node": "Respond to Webhook",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true
}
